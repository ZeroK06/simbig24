---
import Wrapper from '../components/Wrapper.astro'
import DATA from '../../information.json'
const {
  dates: { title, paragraph },
} = DATA
---

<section class="py-20 bg-bg text-white">
  <Wrapper>
    <span class="text-primary font-medium text-base text-center block"
      >[Our Important Dates]</span
    >
    <h2 class="text-center text-5xl font-bold mb-2">{title}</h2>
    <p class="text-center text-white/80 mb-6">{paragraph}</p>
  </Wrapper>
</section>

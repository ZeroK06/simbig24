---
import SocialLinks from '../components/SocialLinks.astro'
import Layout from '../layouts/Layout.astro'
import Hero from '../components/Hero.astro'
import About from '../components/About.astro'
import Speakers from '../components/Speakers.astro'
import Entidades from '../components/Entidades.astro'
import Testimonial from '../components/Testimonial.astro'
import Counters from '../components/Counters.astro'
import CTA from '../components/CTA.astro'
import Dates from '../components/Dates.astro'
import Wrapper from '../components/Wrapper.astro'
---

<Layout title="Welcome to Astro.">
  <SocialLinks />
  <Hero />
  <main>
    <About />
    <Counters />
    <Speakers />
    <Dates />
    <Wrapper>
      <section>Papers</section>
    </Wrapper>
    <Entidades />
    <Testimonial />
    <CTA />
    <Wrapper>
      <section>Contact us</section>
    </Wrapper>
  </main>
</Layout>

import LayoutPage from '../../../layouts/LayoutPage.astro'
import DATA from '../../../../information.json'
import Wrapper from '../../../components/Wrapper.astro'
const {
  'organizing-committee': {
    tag,
    title,
    background,
    items: {
      'general-chair': { items: itemsGeneral, title: titleGeneral },
      'local-chair': { items: itemsLocal, title: titleLocal },
      'web-developer': { items: itemsWeb, title: titleWeb },
      'social-media': { items: itemsSocial, title: titleSocial },
    },
  },
